source /usr/share/nvm/init-nvm.sh
eval "$(zoxide init zsh)"
alias code="/usr/bin/code --enable-features=UseOzonePlatform --enable-features=WaylandWindowDecorations --ozone-platform-hint=auto --unity-launch %F" 
eval "$(oh-my-posh init zsh --config $HOME/.config/oh-my-posh/jags.toml)"
alias py="python3"

export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init --path)"
eval "$(pyenv init -)"


